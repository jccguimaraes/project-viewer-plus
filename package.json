{
  "name": "project-viewer-plus",
  "main": "./dist/index",
  "version": "0.0.1",
  "description": "Manage all your projects into different groups.",
  "keywords": [
    "project",
    "productivity",
    "management",
    "settings",
    "workflow"
  ],
  "repository": "https://github.com/jccguimaraes/project-viewer-plus",
  "license": "MIT",
  "engines": {
    "atom": ">=1.27.1"
  },
  "deserializers": {
    "ProjectViewerPlus": "deserialize"
  },
  "dependencies": {
    "atom-package-deps": "4.6.2",
    "atom-select-list": "0.7.1",
    "etch": "0.14.0",
    "uuid": "3.2.1"
  },
  "devDependencies": {
    "@commitlint/cli": "6.2.0",
    "all-contributors-cli": "4.11.1",
    "atom-babel6-transpiler": "1.1.3",
    "atom-coverage": "1.1.2",
    "atom-mocha": "2.0.5",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.2",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-istanbul": "4.1.6",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "codecov": "3.0.2",
    "cross-env": "5.1.6",
    "eslint": "4.19.1",
    "file-icons": "git+https://github.com/file-icons/atom.git",
    "husky": "0.14.3",
    "nyc": "11.8.0",
    "prettier": "1.13.0",
    "rimraf": "2.6.2",
    "sinon": "5.0.10",
    "sinon-chai": "3.1.0"
  },
  "scripts": {
    "precommit": "npm run commitmsg",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "clean": "rimraf ./dist",
    "icons": "node ./bin/file-icons",
    "contribute": "all-contributors",
    "linter:spec": "eslint ./spec",
    "linter:lib": "eslint ./lib",
    "pretest": "npm run linter:lib",
    "test": "npm run linter:spec",
    "posttest": "cross-env NODE_ENV=test atom --test spec/integration/** spec/unit/**",
    "precompile": "npm run clean",
    "compile": "babel lib --out-dir dist",
    "prewatch": "npm run clean",
    "watch": "babel --watch lib --out-dir dist",
    "test:coverage": "atom-coverage",
    "check:coverage": "nyc check-coverage",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov"
  },
  "atomTranspilers": [
    {
      "glob": "lib/**/*.js",
      "transpiler": "atom-babel6-transpiler",
      "options": {
        "cacheKeyFiles": [
          "package.json",
          ".babelrc"
        ]
      }
    }
  ],
  "atomTestRunner": "atom-mocha",
  "package-deps": [
    "file-icons"
  ]
}
