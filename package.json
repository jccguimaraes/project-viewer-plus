{
  "name": "project-viewer-plus",
  "main": "./dist/index",
  "version": "0.0.4",
  "description": "Manage all your projects into different groups.",
  "keywords": [
    "project",
    "productivity",
    "management",
    "settings",
    "workflow"
  ],
  "repository": "https://github.com/jccguimaraes/project-viewer-plus",
  "license": "MIT",
  "engines": {
    "atom": ">=1.39.1",
    "node": ">=10.2.0"
  },
  "scripts": {
    "clean": "rimraf ./dist",
    "icons": "node ./bin/file-icons",
    "contribute": "all-contributors",
    "linter:spec": "eslint ./spec --fix",
    "linter:lib": "eslint ./lib --fix",
    "pretest": "npm run linter:spec",
    "test": "atom -t spec/",
    "precompile": "npm run clean",
    "compile": "babel lib --out-dir dist",
    "prewatch": "npm run clean",
    "watch": "npm run linter:lib",
    "postwatch": "babel --watch lib --out-dir dist"
  },
  "deserializers": {
    "ProjectViewerPlus": "deserialize"
  },
  "atomTranspilers": [
    {
      "glob": "lib/**/*.js",
      "transpiler": "@atom/babel7-transpiler"
    }
  ],
  "atomTestRunner": "atom-mocha",
  "package-deps": [
    "file-icons"
  ],
  "dependencies": {
    "@atom/babel7-transpiler": "^1.0.0-1",
    "atom-package-deps": "^5.1.0",
    "atom-select-list": "^0.7.2",
    "etch": "^0.14.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/plugin-transform-modules-commonjs": "^7.5.0",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@commitlint/cli": "^8.1.0",
    "all-contributors-cli": "^6.8.1",
    "atom-mocha": "^2.2.1",
    "babel-eslint": "^10.0.2",
    "babel-plugin-add-module-exports": "^1.0.2",
    "chai": "^4.2.0",
    "eslint": "^6.1.0",
    "file-icons": "git+https://github.com/file-icons/atom#v2.1.33",
    "husky": "^3.0.2",
    "rimraf": "^2.6.3",
    "sinon": "^7.4.0"
  }
}
