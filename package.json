{
  "name": "project-viewer-plus",
  "main": "./bundle/main",
  "version": "0.0.0",
  "description": "Manage all your projects into different groups.",
  "keywords": [
    "project",
    "productivity",
    "management",
    "settings",
    "workflow"
  ],
  "repository": "https://github.com/jccguimaraes/project-viewer-plus",
  "license": "MIT",
  "engines": {
    "atom": ">=1.22.0"
  },
  "private": true,
  "dependencies": {
    "atom-select-list": "0.3.2",
    "etch": "0.12.6"
  },
  "deserializers": {
    "ProjectViewer": "deserialize"
  },
  "devDependencies": {
    "all-contributors-cli": "latest",
    "atom-babel6-transpiler": "1.1.2",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.0.1",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-inline-json-import": "0.2.1",
    "babel-plugin-transform-async-to-generator": "6.24.1",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-do-expressions": "6.22.0",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.0",
    "babel-plugin-transform-export-extensions": "6.22.0",
    "babel-plugin-transform-flow-strip-types": "6.22.0",
    "babel-plugin-transform-function-bind": "6.22.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-react": "6.24.1",
    "eslint": "4.9.0",
    "uuid": "^3.1.0"
  },
  "scripts": {
    "contribute": "all-contributors",
    "clean": "rm -rf bundle",
    "compile": "babel lib --out-dir bundle",
    "linter": "eslint ./build",
    "precommit": "npm run test",
    "prepublish": "npm run clean && npm run compile",
    "prepush": "npm run test",
    "test": "apm test",
    "dev:test": "npm run clean && npm run compile && npm run linter && npm run test",
    "watch": "babel --watch lib --out-dir bundle"
  },
  "atomTranspilers": [
    {
      "glob": "lib/**/*.js",
      "transpiler": "atom-babel6-transpiler",
      "options": {
        "cacheKeyFiles": [
          "package.json",
          ".babelrc"
        ]
      }
    }
  ]
}
